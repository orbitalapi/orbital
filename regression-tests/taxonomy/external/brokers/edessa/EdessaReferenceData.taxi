namespace edessa.referencedata {

   type KeplerTraderBrokerLogin inherits bank.common.BankTraderBrokerLogin

   model TraderResolution {
      @Id
      @PrimaryKey
      traderBrokerLogin: edessa.referencedata.KeplerTraderBrokerLogin? by column("TraderId")
      traderId: bank.orders.TraderId? by column("UTCode")
   }
}
