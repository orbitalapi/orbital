namespace persepolis.referencedata {
    type TraditionTraderBrokerLogin inherits bank.common.BankTraderBrokerLogin

    model TraderResolution {
        @Id
        @PrimaryKey
        traderBrokerLogin: persepolis.referencedata.TraditionTraderBrokerLogin? by column("TraderId")
        traderId: bank.orders.TraderId? by column("UTCode")
   }
}