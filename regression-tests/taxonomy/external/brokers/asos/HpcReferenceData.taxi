namespace asos.referencedata{

    type HpcTraderBrokerLogin inherits bank.common.BankTraderBrokerLogin

    model TraderResolution {
        @Id
        @PrimaryKey
        traderBrokerLogin: HpcTraderBrokerLogin? by column("ShortCode")
        traderId: bank.orders.TraderId? by column("UTCode")
    }
}
