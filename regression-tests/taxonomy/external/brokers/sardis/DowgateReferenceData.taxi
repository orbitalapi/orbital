namespace sardis.referencedata{

    type DowgateTraderBrokerLogin inherits bank.common.BankTraderBrokerLogin

    model TraderResolution {
        @Id
        @PrimaryKey
        traderBrokerLogin: DowgateTraderBrokerLogin? by column("NAME")
        traderId: bank.orders.TraderId? by column("UTCode")
    }
}