{
  "name": "vyneyard",
  "version": "1.0.0",
  "private": true,
  "description": "Editor for Vyne",
  "keywords": [
    "angular",
    "components",
    "reusable"
  ],
  "scripts": {
    "start": "ng serve",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "postinstall": "webdriver-manager update && ./scripts/protractor.sh",
    "start-api": "cd mock-api && covalent-data &",
    "stop-api": "pkill covalent-data",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "test:watch": "ng test --code-coverage"
  },
  "engines": {
    "node": ">4.4 < 7",
    "npm": ">3"
  },
  "license": "private",
  "contributors": [],
  "dependencies": {
    "@angular/animations": "^4.2.0",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.2.0",
    "@angular/compiler": "^4.2.0",
    "@angular/core": "^4.2.0",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "^4.2.0",
    "@angular/http": "^4.2.0",
    "@angular/material": "2.0.0-beta.10",
    "@angular/platform-browser": "^4.2.0",
    "@angular/platform-browser-dynamic": "^4.2.0",
    "@angular/platform-server": "^4.2.0",
    "@angular/router": "^4.2.0",
    "@covalent/code-editor": "^1.0.0-beta.2",
    "@covalent/core": "1.0.0-beta.7",
    "@covalent/dynamic-forms": "1.0.0-beta.7",
    "@covalent/highlight": "1.0.0-beta.7",
    "@covalent/http": "1.0.0-beta.7",
    "@covalent/markdown": "1.0.0-beta.7",
    "@swimlane/ngx-charts": "^5.3.0",
    "@swimlane/ngx-charts-dag": "git://github.com/swimlane/ngx-charts-dag#3672159e830ca468dbf548eb10a442ea3c31a505",
    "core-js": "^2.4.1",
    "d3": "^4.9.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.10.0",
    "rxjs": "^5.4.0",
    "showdown": "1.6.4",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.0",
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "2.53.36",
    "@types/d3-shape": "^1.2.1",
    "codelyzer": "~3.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.6.0",
    "karma-spec-reporter": "^0.0.31",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "~5.1.0",
    "ts-node": "~2.1.0",
    "tslint": "^5.2.0",
    "typescript": "~2.3.1"
  },
  "optionalDependencies": {
    "@covalent/data": "0.7.0"
  }
}
